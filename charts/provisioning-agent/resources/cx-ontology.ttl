    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_information_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Drive_wheel>,
        schema:driveWheelConfiguration ;
    rdfs:subPropertyOf cx:type ;
    skos:altLabel "drive wheel configuration"@en,
        "driveline"@en ;
    skos:definition "The drive wheel configuration, i.e. which roadwheels will receive torque from the vehicle's engine via the drivetrain."@en ;
    skos:example "all"@en,
        "front"@en,
        "rear"@en ;
    skos:prefLabel "Drive Wheel Type"@en .

cx:euEnergyLabel a owl:DatatypeProperty ;
    rdfs:domain cx:VehicleFuelInformation ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_information_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/European_Union_energy_label#Cars> ;
    skos:altLabel "Energieeffizienzklasse"@de,
        "Energieverbrauchskennzeichnung"@de,
        "Kennzeichnung des Energieverbrauchs"@de,
        "energy efficiency class"@en ;
    skos:example "A"@en,
        "B"@en,
        "C"@en,
        "D"@en ;
    skos:prefLabel "Eu Energy Label"@en .

cx:vehicleIdentificationNumber a owl:DatatypeProperty ;
    rdfs:domain cx:Vehicle ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Vehicle_identification_number>,
        schema:vehicleIdentificationNumber ;
    rdfs:subPropertyOf cx:number ;
    skos:altLabel "FIN"@de,
        "Fahrgestellnummer"@de,
        "Fahrzeugidentifikationsnummer"@de,
        "VIN"@en,
        "vehicle identification number"@en ;
    skos:definition "The Vehicle Identification Number  is a unique serial number used by the automotive industry to identify individual motor vehicles."@en ;
    skos:note "17-digit unique identifier"@en ;
    skos:prefLabel "Fahrzeug-Identifizierungsnummer"@de,
        "Vehicle Identification Number"@en .

cx:vehicleModel a owl:DatatypeProperty ;
    rdfs:domain cx:VehicleModel ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_information_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Car_model>,
        schema:ProductModel,
        <https://www.wikidata.org/wiki/Q29048322> ;
    skos:altLabel "base model"@en ;
    skos:example "320"@en,
        "C220"@en,
        "Golf"@en,
        "Polo"@en ;
    skos:prefLabel "Fahrzeugmodell"@de,
        "Model"@en .

cx:vehicleRegistrationPlate a owl:DatatypeProperty ;
    rdfs:domain cx:VehicleRegistrationInformation ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_information_ontology.ttl> ;
    rdfs:range xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Vehicle_registration_plate> ;
    skos:altLabel "Kfz-Kennzeichen"@de,
        "Kraftfahrzeugkennzeichen"@de,
        "Nummernschild"@de,
        "licence plate"@en,
        "license plate"@en,
        "number plate"@en,
        "vehicle registration number"@en ;
    skos:prefLabel "Kennzeichen"@de,
        "License plate"@en .

cx:LoadSpectrum a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/load_spectrum_ontology.ttl> ;
    rdfs:seeAlso <https://de.wikipedia.org/wiki/Lastkollektiv>,
        <https://en.wikipedia.org/wiki/Mechanical_load> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:hasLoadSpectrumChannel ;
            owl:someValuesFrom cx:LoadSpectrumChannel ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumDescription ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumId ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumStartDatetime ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumType ;
            owl:someValuesFrom json:Object ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumName ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumEndDatetime ;
            owl:someValuesFrom xsd:dateTime ],
        [ a owl:Restriction ;
            owl:onProperty cx:hasLoadSpectrumValues ;
            owl:someValuesFrom cx:LoadSpectrumValues ],
        [ a owl:Restriction ;
            owl:onProperty cx:loadSpectrumDescription ;
            owl:someValuesFrom xsd:string ],
        cx:ImmaterialThing ;
    skos:altLabel "load collective"@en ;
    skos:definition "Load spectrum is a 2d histogram that contains the load history of a vehicle, i.e. how a vehicle was used, for a given time period."@en ;
    skos:example "force"@en,
        "tension"@en,
        "torque"@en ;
    skos:prefLabel "Lastkollektiv"@de,
        "Load Spectrum"@en .

cx:Truck a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Truck> ;
    rdfs:subClassOf cx:CommercialVehicle,
        cx:HeavyTransportVehicle ;
    skos:altLabel "Güterkraftfahrzeug"@de,
        "LKW"@de,
        "Lastkraftwagen"@de,
        "lorry"@en ;
    skos:prefLabel "Lastkraftwagen"@de,
        "Truck"@en .

cx:CommercialVehicle a owl:Class ;
    rdfs:isDefinedBy <https://raw.githubusercontent.com/catenax-ng/product-knowledge/main/ontology/vehicle_ontology.ttl> ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Commercial_vehicle> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cx:isOwnedBy ;
            owl:someValuesFrom cx:Organisation ],
        cx:Vehicle ;
    skos:altLabel "NFZ"@de,
        "NKW"@de,
        "Nutzfahrzeug"@de,
        "Nutzkraftwagen"@de ;
    skos:definition "A vehicle that transports people or cargo for payment."@en ;
    skos:note "> 9 passengers"@en ;
    skos:prefLabel "Nutzfahrzeug"@de,
        "Commercial Vehicle"@en .

